  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.21.3",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "flutterwave-node": "^1.2.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mongoose": "5.10.5",
    "multer": "^1.4.2",
    "node-geocoder": "^3.27.0",
    "rimraf": "^3.0.2",
    "slugify": "^1.5.3",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "concurrently": "^6.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  }
